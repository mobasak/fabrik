{
  "model": "gpt-5.1-codex-max",
  "reasoningEffort": "high",
  "autonomyLevel": "auto-high",
  "diffMode": "github",
  "cloudSessionSync": true,
  "specSaveEnabled": true,
  "specSaveDir": ".factory/docs",
  "allowBackgroundProcesses": true,
  "enableDroidShield": false,
  "todoDisplayMode": "pinned",
  "completionSound": "fx-ok01",
  "awaitingInputSound": "fx-ack01",
  "soundFocusMode": "always",
  "includeCoAuthoredByDroid": true,
  "enableCustomDroids": true,
  "enableReadinessReport": true,
  "enableHooks": true,
  "commandAllowlist": [
    "ls",
    "pwd",
    "dir",
    "cat",
    "head",
    "tail",
    "grep",
    "find",
    "git push",
    "git commit",
    "git pull",
    "git add",
    "git status",
    "git diff",
    "git log",
    "git show",
    "git blame",
    "git ls-files",
    "git checkout",
    "git branch",
    "git merge",
    "git rebase",
    "pip install",
    "pip3 install",
    "uv pip install",
    "uv sync",
    "npm install",
    "npm test",
    "npm run",
    "yarn install",
    "yarn test",
    "pnpm install",
    "pnpm test",
    "python",
    "python3",
    "node",
    "make",
    "cargo build",
    "go build",
    "docker build",
    "docker run",
    "docker compose",
    "docker compose up",
    "docker compose down",
    "docker compose logs",
    "pytest",
    "ruff",
    "ruff check",
    "ruff format",
    "mypy",
    "sh",
    "bash",
    "curl",
    "wget",
    "rm -rf /opt",
    "rm -rf /opt/*",
    "rm -rf .tmp",
    "rm -rf .cache",
    "rm -rf __pycache__",
    "rm -rf node_modules",
    "rm -rf .venv",
    "cd /opt",
    "cd /opt/*",
    "uvicorn",
    "gunicorn",
    "alembic",
    "prisma"
  ],
  "commandDenylist": [
    "rm -rf /",
    "rm -rf /*",
    "rm -r /",
    "rm -r /*",
    "rm -rf .",
    "rm -rf ~",
    "rm -rf ~/*",
    "rm -rf /etc",
    "rm -rf /etc/*",
    "rm -rf /usr",
    "rm -rf /usr/*",
    "rm -rf /bin",
    "rm -rf /sbin",
    "rm -rf /lib",
    "rm -rf /var",
    "rm -rf /boot",
    "rm -rf /root",
    "rm -rf /home",
    "rm -rf /home/*",
    "mkfs",
    "mkfs.ext4",
    "mkfs.ext3",
    "dd if=/dev/zero of=/dev",
    "dd of=/dev",
    ":(){ :|: & };:",
    "shutdown",
    "reboot",
    "halt",
    "poweroff",
    "init 0",
    "init 6",
    "chmod -R 777 /",
    "chmod -R 000 /",
    "chown -R root:root /",
    "rm -rf $HOME",
    "format"
  ]
}
